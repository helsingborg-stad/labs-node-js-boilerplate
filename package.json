{
  "name": "helsingborg-io-boilerplate",
  "version": "0.0.2-beta.2",
  "description": "Boilerplate repository for nodejs based LABS projects.",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production node src/server.js",
    "dev": "nodemon src/server.js | ./node_modules/.bin/pino-colada",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha src/components/**/*.test.js",
    "test:watch": "./node_modules/.bin/mocha src/components/**/*.test.js --watch",
    "lint": "./node_modules/.bin/eslint src/**/*.js",
    "migrate:latest": "./node_modules/.bin/knex --cwd src/db/ migrate:latest",
    "migrate:rollback": "./node_modules/.bin/knex --cwd src/db/ migrate:rollback",
    "setup": "node setup.js"
  },
  "author": "Helsingborg Stad",
  "license": "MIT",
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-asserttype": "^1.0.5",
    "chai-http": "^4.3.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-prettier": "^3.1.1",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.0",
    "pino-colada": "^1.4.4",
    "prettier": "1.18.2"
    "nyc": "^14.1.1",
  },
  "dependencies": {
    "@hapi/joi": "^15.0.1",
    "axios": "^0.18.1",
    "body-parser": "^1.19.0",
    "bookshelf": "^0.15.1",
    "dotenv": "^8.0.0",
    "easy-soap-request": "^2.5.0",
    "express": "^4.16.4",
    "express-pino-logger": "^4.0.0",
    "json-api-serializer": "^2.1.0",
    "json-schema-ref-parser": "^7.1.1",
    "knex": "^0.17.1",
    "mysql": "^2.17.1",
    "pino": "^5.12.5",
    "redis": "^2.8.0",
    "soap": "^0.27.1",
    "swagger-ui-express": "^4.0.2",
    "ws": "^7.0.0"
  }
}
